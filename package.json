{
  "name": "chess-with-react-redux",
  "version": "1.0.0",
  "main": "src/index.jsx",
  "repository": "https://github.com/luddwichr/chess-with-react-redux.git",
  "author": "LR",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel public/index.html --open",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:lint": "eslint src/**/*.js[x] test-setup.js jest.config.js --fix",
    "build:local": "parcel build public/index.html --no-minify",
    "build": "parcel build public/index.html --no-source-maps --experimental-scope-hoisting",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">= 10.*",
    "yarn": ">= 1.13.*"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-css-modules": "^1.1.0",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^8.1.2",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-clean-dist": "^0.0.6",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "react-test-renderer": "^16.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "pretty-quick --staged !package.json",
      "eslint src/**/*.js[x] test-setup.js jest.config.js --fix",
      "git add"
    ]
  }
}
